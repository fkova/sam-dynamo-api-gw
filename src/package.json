{
  "name": "sam-dynamo-apigw",
  "version": "1.0.0",
  "description": "",
  "main": "../handler.js",
  "scripts": {
    "start": "npm run build && npm run package && npm run deploy",
    "test": "jest unit",
    "create-bucket": "aws s3 mb s3://rahzel-package --region us-east-2",
    "build": "sam build -t ../sam.yml",
    "build2": "webpack",
    "package": "sam package --output-template template.yaml --s3-bucket rahzel-package",
    "deploy": "sam deploy --template-file template.yaml --region us-east-2 --capabilities CAPABILITY_IAM --stack-name sam-dynamo-apigw",
    "delete-stack": "aws --region us-east-2 cloudformation delete-stack --stack-name sam-dynamo-apigw"
  },
